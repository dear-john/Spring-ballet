//直接统计每一个字符串的字母数目，用string连接会超出内存

#include<iostream>
#include<string>
#include<string.h>
using namespace std;
int main(){
  int k;
  int t;
  cin>>t;
  long long a[26];
  long long b[26];
  long long c[26];
  char s1[40],s2[40];
  int i;
  while(t--){
    cin>>s1>>s2;
    cin>>k;
    for(i=0;i<26;i++){
        a[i]=b[i]=0;
    }
    for(i=0;i<strlen(s1);i++)
        a[s1[i]-'a']++;
    for(i=0;i<strlen(s2);i++)
        b[s2[i]-'a']++;
    if(k>1){
        k--;
        while(k--){
           for(i=0;i<26;i++){
                c[i]=a[i]+b[i];
                a[i]=b[i];
                b[i]=c[i];
            }
        }
    }else if(k==0){
        for(i=0;i<26;i++)
            c[i]=a[i];
    }else {
        for(i=0;i<26;i++)
            c[i]=b[i];
    }
    for(i=0;i<26;i++){
        cout<<(char)(97+i)<<":"<<c[i]<<endl;
    }
    cout<<endl;
  }
  return 0;
}
