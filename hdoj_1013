//上面的代码是按题目直接做 
//下面的是 九余数法   
#include<iostream>
#include<string.h>
using namespace std;
const int T=1000;

int main()
{
	
		char n[T];
		
		while(cin>>n&&n[0]!='0')
		{
			int sum=0;
			for(int i=0;i<strlen(n);i++)
				sum+=n[i]-'0';
		
		if(!sum)return 0;
		if(sum%9==0)cout<<'9'<<endl;
		else cout<<sum%9<<endl;
		}
	return 0; 
 } 
