//神奇的格式，每行最后有空格，最后一行之后无需换行，逗号之前没有空格，两行数据间有两个空行
//打表做法，下面有别人的不打表做法
#include<iostream>
using namespace std;
int prime[65540]={-1,-1,2};
void fun(){
	int i,j;
	for(i=2;i<65540;i++){
		if(prime[i]!=-1){
			prime[i]=i;
			for(j=2;j*i<65540;j++)
                prime[i*j]=-1;
		}
	}
}
int main(){
	fun();
	int n;
	int i;
	int sum;
	int order=0;
    int temp;
	while(cin>>n){
        if(n<0)break;
        if(order!=0)cout<<endl;
		cout<<"Case "<< ++order <<".\n";
		if(prime[n]!=-1){
			cout<<n<<" "<<1<<" "<<endl;
		}else{
			temp=n/2;
			for(i=2;i<=temp;i++){
	            if(prime[i]!=-1){
	                sum=0;
	                while(n%prime[i]==0){
						sum++;
						n/=prime[i];
					}
					if(sum){
						cout<<prime[i]<<" "<<sum<<" ";
	                	if(n==1) break;
					}
				}
	    	}
	    	cout<<endl;
		}
	}
	return 0;
}

//不打表做法

#include<iostream>
using namespace std;
int main(){
	int n;
	int i;
	int sum;
	int order=0;
	while(cin>>n){
        if(n<0)break;
        if(order!=0)cout<<endl;
		cout<<"Case "<< ++order <<".\n";
		if(n==1)cout<<"1 1 ";
		for(i=2;i*i<=n;i++){
			sum=0;
			while(n%i==0){
				sum++;
				n/=i;
			}
			if(sum)cout<<i<<" "<<sum<<" ";
		}
		if(n!=1)
			cout<<n<<" "<<1<<" ";
	    cout<<endl;
	}
	return 0;
}
