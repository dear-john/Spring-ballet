#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
	int step,mod,i;
	bool flag=false;
	while(cin>>step>>mod)
	{
		int max=step>mod?step:mod;
		for(i=2;i<=max;i++)
		{
			if(step%i==0&&mod%i==0)///求公约数 
			{
				flag=false;
				break;
			}
		}
		if(i>max)flag=true;
		cout<<setw(10)<<step<<setw(10)<<mod;
		if(flag)
			cout<<"    Good Choice\n\n";
		else
			cout<<"    Bad Choice\n\n"; 
	}
	return 0;
 } 
//和桶排序类似的想法

int main()
{

	//
	int step;
	int mod;
	int temp;
	bool flag;
	int num[100005];
	while(cin>>step>>mod){
		for(int i=0;i<mod;i++)
			num[i]=0;
		temp=0;
		for(int i=1;i<mod;i++){
			flag=false;
			temp=(temp+step)%mod;
			if(num[temp]!=0){
				flag=true;
				break;
			}
			else num[temp]=1;
		}
		if(flag){
			cout<<setw(10)<<step
				<<setw(10)<<mod
				<<"    Bad Choice\n\n";
		}else{
			cout<<setw(10)<<setiosflags(ios::right)<<step
				<<setw(10)<<setiosflags(ios::right)<<mod
				<<"    Good Choice\n\n";
		}
	}
