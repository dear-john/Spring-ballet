#include<iostream>
#include<string.h>
using namespace std;

int main()
{
	int T;
	cin>>T;
	
	for(int i=1; i<=T; i++ )
	{
		char x[1000],y[1000];
		int a[1001]={0},b[1001]={0};
		cin>>x>>y;			//or直接倒着存 
		
		int lenx=strlen(x);
		int leny=strlen(y);
		int maxlen=(lenx>leny?lenx:leny);
		
		cout<<"Case "<<i<<':'<<endl;
		cout<<x<<" + "<<y<<" = ";
		
		for(int j=0,i=lenx-1;i>=0;j++,i--)
		{
			a[j]=x[i]-'0';
		}
		for(int j=0,i=leny-1;i>=0;j++,i--)
		{
			b[j]=y[i]-'0';
		}
		
		for(int k=0;k<=maxlen;k++)
		{
			a[k]+=b[k];
			if(a[k]>9)
			{
				a[k]-=10;
				a[k+1]++;
			}
		}
		
		int t;
		for(t=maxlen;a[t]==0;t--);	
		for(;t>=0;t--)
		{
			cout<<a[t];
		}
		cout<<endl;
		if(i!=T)
			cout<<endl;
	}
	
	return 0;
 } 
