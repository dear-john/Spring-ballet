#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	double e = 2.5,s = 2; 
	cout << "n e\n";
	cout << "- -----------\n";
		
	for (int i = 0; i <= 9; i++)
	{
		if(i == 0) cout << "0 1\n";
		if(i == 1)cout << "1 2\n";
		if(i == 2)cout << "2 "<<e<<endl;
		if(i > 2)
		{
			s*=i;
			e += (double) (1/s);
			cout << i << ' ' 
			<< fixed << setprecision(9) << e<<endl;
		}
	}
	return 0;
}
