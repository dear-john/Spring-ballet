#include<iostream>
using namespace std;

int maxgys(int a,int b)
{
	if(a==b)return a;
	if(a<b)
	{
		int temp=a;
		a=b;
		b=temp;
	}
	for(int i=b;i>1;i--)
	{
		if(a%i==0&&b%i==0)
			return i;
	}
	return 1;
}

int main()
{
	int N,num;
	cin>>N;
	while(N--)
	{
		cin>>num;
		int m=1,ans,a;
		for(int i=0;i<num;i++)
		{
			cin>>a;	
			ans=a/maxgys(a,m)*m;
			m=ans;
		}
		cout<<ans<<endl;
	}
	return 0;
 } 
