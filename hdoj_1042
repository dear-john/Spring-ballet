#include<iostream>
using namespace std;
const int N=36000;
int main()
{
	int n,i;
	while(cin>>n)
	{
		int a[N]={1};
		for(i=2;i<=n;i++)
		{
			int c=0;
			for(int j=0;j<N;j++)
			{
				int s=a[j]*i+c;
				a[j]=s%10;
				c=s/10;	
			}
		}
		for(i=N-1;i>=0;i--)
			if(a[i])
				break;
		for(int j=i;j>=0;j--)
			cout<<a[j];
		cout<<endl;
	}
	return 0;
 } 
