//自定义sort函数的比较函数，分为++，+-，--三种情况的比较

#include<iostream>
#include<algorithm>
#include<iomanip>
using namespace std;
struct Point{
  double x;
  double y;
  double rec;
};
bool cmp(Point a,Point b){
    if(b.rec>=0&&a.rec>=0){
        return a.rec>b.rec?false:true;
    }else if(b.rec<0&&a.rec<0){
        return a.rec>b.rec?false:true;
    }else return a.rec>b.rec?true:false;
}
int main(){
  Point point[105];
  int n;
  int i;
  while(cin>>n&&n>0){
    for(i=0;i<n;i++){
        cin>>point[i].x>>point[i].y;
        point[i].rec=point[i].y*1.0/point[i].x;
    }
    sort(point,point+n,cmp);
    for(i=0;i<n;i++){
        cout<<fixed<<setprecision(1)<<point[i].x<<" "<<point[i].y;
        if(i!=n-1)cout<<" ";
    }
    cout<<endl;
  }
  return 0;
}
